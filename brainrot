local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

-- Guardar posici√≥n base
local basePosition = nil

-- Funci√≥n para destruir sistemas de correcci√≥n del juego
local function bypassTP()
    -- Intenta destruir scripts locales que puedan corregir la posici√≥n
    for _, v in pairs(char:GetDescendants()) do
        if v:IsA("LocalScript") or v:IsA("Script") then
            pcall(function()
                v:Destroy()
            end)
        end
    end
    -- Eliminar estados f√≠sicos que impidan moverse
    if char:FindFirstChild("Humanoid") then
        char.Humanoid:ChangeState(11)
    end
end

-- Crear UI
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "BypassTPGui"

local save = Instance.new("TextButton", gui)
save.Size = UDim2.new(0, 160, 0, 40)
save.Position = UDim2.new(0, 20, 0, 100)
save.Text = "üìç Guardar Base"
save.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
save.TextColor3 = Color3.fromRGB(255, 255, 255)
save.Font = Enum.Font.SourceSansBold
save.TextSize = 18

local tp = Instance.new("TextButton", gui)
tp.Size = UDim2.new(0, 160, 0, 40)
tp.Position = UDim2.new(0, 20, 0, 150)
tp.Text = "üöÄ Teleport Forzado"
tp.BackgroundColor3 = Color3.fromRGB(80, 0, 0)
tp.TextColor3 = Color3.fromRGB(255, 255, 255)
tp.Font = Enum.Font.SourceSansBold
tp.TextSize = 18

-- Guardar base
save.MouseButton1Click:Connect(function()
    char = player.Character or player.CharacterAdded:Wait()
    hrp = char:FindFirstChild("HumanoidRootPart")
    if hrp then
        basePosition = hrp.Position
        save.Text = "‚úÖ Guardado"
        wait(1)
        save.Text = "üìç Guardar Base"
    end
end)

-- Teleport con bypass
tp.MouseButton1Click:Connect(function()
    char = player.Character or player.CharacterAdded:Wait()
    hrp = char:FindFirstChild("HumanoidRootPart")

    if basePosition and hrp then
        bypassTP() -- Intento de desactivar correcciones

        -- Mover pieza por pieza
        for i = 1, 10 do
            hrp.CFrame = CFrame.new(basePosition + Vector3.new(0, 5, 0))
            wait(0.1) -- repetici√≥n para forzar el TP
        end
    else
        tp.Text = "‚ö†Ô∏è No hay base guardada"
        wait(1.5)
        tp.Text = "üöÄ Teleport Forzado"
    end
end)
